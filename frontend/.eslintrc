{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "quotes": [2, "single", { "avoidEscape": true }]
  },
  "ignorePatterns": ["out/", "node_modules/"]
}