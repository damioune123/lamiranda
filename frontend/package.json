{
  "name": "nextjs-react-redux-filter",
  "version": "1.0.0",
  "main": "next-server/index.js",
  "repository": "https://github.com/yrfang/nextjs-react-redux-filter.git",
  "author": "Damien Meur <damienmeur@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch": "sh -ac 'cp .env.${REACT_APP_ENV} .env;  nodemon next-server/index.js'",
    "watch:local": "REACT_APP_ENV=local npm run watch",
    "export": "sh  -ac 'cp .env.${REACT_APP_ENV} .env;  next export'",
    "export:staging": "REACT_APP_ENV=staging npm run export",
    "export:prod": "REACT_APP_ENV=prod npm run export",
    "export:local": "REACT_APP_ENV=local npm run export",
    "build": "sh  -ac 'cp .env.${REACT_APP_ENV} .env;  next build'",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "build:local": "REACT_APP_ENV=local npm run build",
    "start": "sh  -ac 'cp .env.${REACT_APP_ENV} .env;  node next-server/index.js'",
    "start:staging": "REACT_APP_ENV=staging npm run start",
    "start:prod": "REACT_APP_ENV=prod npm run start",
    "start:local": "REACT_APP_ENV=local npm run start"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@directus/sdk-js": "^6.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "express": "^4.17.2",
    "immutable": "^4.0.0",
    "next": "^12.0.7",
    "next-export-i18n": "^1.2.1",
    "next-redux-saga": "^5.0.0-0",
    "next-redux-wrapper": "^7.0.5",
    "next-routes": "^1.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazy-load": "^3.1.13",
    "react-loader-spinner": "^4.0.0",
    "react-redux": "^7.2.6",
    "recompose": "^0.30.0",
    "redux": "^4.1.2",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.28.0",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.15",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0"
  }
}
